            <sponsorimages
                jcr:primaryType="cq:Widget"
                title="Sponsor Images"
                xtype="panel">
                <items jcr:primaryType="cq:WidgetCollection">
                    <sponsor1
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Sponsor 1"
                        name="./sponsor1"
                        rootPath="/content/dam/nhmwww"
                        precidate="nosystem"
                        xtype="pathfield"/>
                    <sponsor2
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Sponsor 2"
                        name="./sponsor2"
                        rootPath="/content/dam/nhmwww"
                        precidate="nosystem"
                        xtype="pathfield"/>
                    <sponsor3
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Sponsor 3"
                        rootPath="/content/dam/nhmwww"
                        precidate="nosystem"
                        name="./sponsor3"
                        xtype="pathfield"/>
                    <sponsor4
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Sponsor 4"
                        rootPath="/content/dam/nhmwww"
                        precidate="nosystem"
                        name="./sponsor4"
                        xtype="pathfield"/>
                </items>
            </sponsorimages>
        </items>
    </items>
</jcr:root>
